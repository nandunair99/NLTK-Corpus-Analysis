    "    tree = ET.parse(file_path)\n",
    "    root = tree.getroot()\n",
    "    \n",
    "    sentences = []\n",
    "    corpus_text=\"\"\n",
    "    \n",
    "    # Traverse the XML structure to extract text\n",
    "    for document in root.findall('text'):\n",
    "        text_element = document.find('rohtext')\n",
    "        if text_element is not None:\n",
    "            text = text_element.text\n",
    "            corpus_text=corpus_text+\" \"+text\n",
    "    \n",
    "    return corpus_text\n",
    "\n",
