      "Split Subwords: []\n"
     ]
    }
   ],
   "source": [
    "from nltk.corpus import words\n",
    "\n",
    "all_words = nltk.word_tokenize(text)\n",
    "# Get the list of English words from NLTK\n",
    "word_list = set(words.words())\n",
    "\n",
    "longest_word = max(all_words, key=len)\n",
    "def word_break(s, word_dict):\n",
    "    # Initialize the dp array where dp[i] is True if s[:i] can be segmented\n",
    "    dp = [False] * (len(s) + 1)\n",
    "    dp[0] = True  # An empty string can always be segmented\n",
    "    \n",
    "    # To store the words that form s[:i]\n",
    "    subwords = [[] for _ in range(len(s) + 1)]\n",
    "    \n",
    "    for i in range(1, len(s) + 1):\n",
    "        for j in range(i):\n",
    "            # Check if s[j:i] is a valid word and if s[:j] can be segmented\n",
    "            if dp[j] and s[j:i] in word_dict:\n",
    "                dp[i] = True\n",
    "                subwords[i] = subwords[j] + [s[j:i]]\n",
    "                break\n",
    "    \n",
    "    # If the entire word can be segmented, return the subwords\n",
    "    return subwords[-1] if dp[-1] else []\n",
    "\n",
    "split_subwords = word_break(longest_word, word_list)\n",
    "\n",
    "print(\"Longest Word:\", longest_word)\n",
    "print(\"Split Subwords:\", split_subwords)\n"
   ]
